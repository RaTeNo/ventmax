.checkbox__input {
  position: absolute;
  z-index: -1;
  opacity: 0;
  visibility: hidden;
}
.checkbox__input + label {
  position: relative;
  padding-left: 36px;
  cursor: pointer;
}
.checkbox__label {
  font-size: 16px;
  line-height: 19px;
  color: #555555;
  margin-bottom: 41px;
}
.checkbox__input + label:before {
  content: "";
  position: absolute;
  top: 2px;
  left: 9px;
  width: 16px;
  height: 16px;
  border: 1px solid #D7D7D7;
  border-radius: 50%;
  background: #ffffff;
  -webkit-transition: 0.2s;
  -o-transition: 0.2s;
  transition: 0.2s;
}
.checkbox__input:checked + label:before {
  content: "";
  position: absolute;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  background: #0088CE;
  top: 2px;
  left: 10px;
  -webkit-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
} 






